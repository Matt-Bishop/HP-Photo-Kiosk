<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
android:id="@+id/widget94"
android:layout_width="fill_parent"
android:layout_height="fill_parent"
xmlns:android="http://schemas.android.com/apk/res/android"
>
<ImageView
android:id="@+id/widget148"
android:layout_width="230dip"
android:layout_height="230dip"
android:src="@drawable/hp_title_rps2"
android:layout_centerVertical="true"
android:layout_alignParentLeft="true"
>
</ImageView>
<TextView
android:id="@+id/widget147"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:text="HP PHOTOKIOSK INTERFACE APP"
android:layout_alignParentTop="true"
android:layout_toRightOf="@+id/widget78"
>
</TextView>

<ImageButton
android:id="@+id/connect"
android:layout_width="300dip"
android:layout_height="60dip"
android:src="@drawable/hp_connect_button"
android:layout_above="@+id/help"
android:layout_alignParentRight="true"
>
</ImageButton>
<ImageButton
android:id="@+id/help"
android:layout_width="300dip"
android:layout_height="60dip"
android:src="@drawable/hp_help_button"
android:layout_above="@+id/exit"
android:layout_alignParentRight="true"
>
</ImageButton>
<ImageButton
android:id="@+id/exit"
android:layout_width="300dip"
android:layout_height="60dip"
android:src="@drawable/hp_exit_button"
android:layout_alignParentBottom="true"
android:layout_alignParentRight="true"
>
</ImageButton>

</RelativeLayout>
